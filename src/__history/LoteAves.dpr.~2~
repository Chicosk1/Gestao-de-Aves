  uLote          in 'Model\Entidades\uLote.pas',
  uPesagem       in 'Model\Entidades\uPesagem.pas',
  uMortalidade   in 'Model\Entidades\uMortalidade.pas',
  dmConexao      in 'Model\Data\dmConexao.pas' {DataModule1: TDataModule},
  uDmInterface   in 'Model\Data\uDmInterface.pas',
  uDmBase        in 'Model\Data\uDmBase.pas',
  uDmLote        in 'Model\Data\uDmLote.pas',
  uDmPesagem     in 'Model\Data\uDmPesagem.pas',
